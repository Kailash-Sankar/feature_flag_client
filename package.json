{
  "name": "react_boilerplate",
  "version": "1.0.0",
  "description": "React application boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "start": "NODE_ENV=development webpack-dev-server --open --config webpack.dev.js --host 0.0.0.0 --port 8080",
    "lint-check": "eslint 'src/**/*.{js,ts,jsx,tsx}'",
    "lint-fix": "eslint --fix 'src/**/*.{js,ts,jsx,tsx}'",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "scan": "npm audit && npm outdated",
    "comply": "license-checker --summary",
    "comply:prod": "license-checker --summary --production",
    "check-types": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-fix"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^3.25.1",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "echarts": "^4.4.0",
    "memoize-one": "^5.1.1",
    "moment-timezone": "^0.5.27",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-typescript": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@types/classnames": "^2.2.9",
    "@types/echarts": "^4.4.1",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.147",
    "@types/moment-timezone": "^0.5.12",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.1",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-react": "^7.16.0",
    "extract-css-chunks-webpack-plugin": "^4.6.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "license-checker": "^25.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  }
}
